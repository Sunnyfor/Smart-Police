    android:id="@+id/main_bottom_graph"
    app:startDestination="@id/logistics_dest">

    <fragment
        android:id="@+id/logistics_dest"
        android:name="com.zhkj.smartpolice.app.fragment.LogisticsFragment"
        tools:layout="@layout/frag_logistics" />

    <fragment
